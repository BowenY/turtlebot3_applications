      turtle_name : "turtlebotA"
      turtle_name_2 : "turtleA_behind"
    </rosparam>
  </node>

  <node pkg="turtlebot02_follower" type="turtlebotA_subscriber" name="turtlebotB_subscriber" output="screen">
