    </rosparam>
  </node>

  <node pkg="turtlebot02_follower" type="turtlebotB_subscriber" name="turtlebotC_subscriber" output="screen">
    <rosparam>
        <!-- turtle_name : "turtlebotB"
        turtle_name_2 : "turtleB_behind" -->
        btw_dist    : -0.4
    </rosparam>
  </node>

  <node pkg="turtlebot02_follower" type="turtlebot02_follower" name="turtlebot02_follower" output="screen">
  </node>

  <node pkg="turtlebot02_follower" type="turtlebot03_follower" name="turtlebot03_follower" output="screen">
  </node>
</launch>
