<launch>
  <node pkg="turtlebot_follow_turtlebot" type="turtlebot_tf_broadcaster" name="turtlebotA_tf_broadcaster" output="screen">
    <rosparam>
      turtle_name   : "turtlebotA"
      turtle_name_2 : "turtleA_behind"
    </rosparam>
  </node>

  <node pkg="turtlebot_follow_turtlebot" type="turtlebot_tf_broadcaster" name="turtlebotB_tf_broadcaster" output="screen">
    <rosparam>
        turtle_name   : "turtlebotB"
        turtle_name_2 : "turtleB_behind"
        btw_dist      : -0.3
    </rosparam>
  </node>

  <node pkg="turtlebot_follow_turtlebot" type="turtlebot_tf_broadcaster" name="turtlebotC_tf_broadcaster" output="screen">
    <rosparam>
        turtle_name   : "turtlebotC"
        turtle_name_2 : "turtleC_behind"
        btw_dist      : -0.6
    </rosparam>
  </node>

  <node pkg="turtlebot_follow_turtlebot" type="turtlebot_tf_broadcaster" name="turtlebotD_tf_broadcaster" output="screen">
    <rosparam>
        turtle_name   : "turtlebotD"
        turtle_name_2 : "turtleD_behind"
        btw_dist      : -0.9
    </rosparam>
  </node>

  <!-- <node pkg="turtlebot_follow_turtlebot" type="turtlebot_tf_broadcaster" name="turtlebotD_tf_broadcaster" output="screen">
    <rosparam>
        turtle_name   : "turtlebotE"
        turtle_name_2 : "turtleE_behind"
        btw_dist      : -0.8
    </rosparam>
  </node> -->

  <node pkg="turtlebot_follow_turtlebot" type="turtlebot_follow_turtlebot" name="turtlebotB_follow_turtlebotA" output="screen">
    <rosparam>
        turtlebot   : "turtlebotB"
        target_turtlebot : "turtlebotA"
        target_turtlebot_behind : "turtleA_behind"
    </rosparam>
  </node>

  <node pkg="turtlebot_follow_turtlebot" type="turtlebot_follow_turtlebot" name="turtlebotC_follow_turtlebotB" output="screen">
    <rosparam>
        turtlebot   : "turtlebotC"
        target_turtlebot : "turtlebotB"
        target_turtlebot_behind : "turtleB_behind"
    </rosparam>
  </node>

  <node pkg="turtlebot_follow_turtlebot" type="turtlebot_follow_turtlebot" name="turtlebotD_follow_turtlebotC" output="screen">
    <rosparam>
        turtlebot   : "turtlebotD"
        target_turtlebot : "turtlebotC"
        target_turtlebot_behind : "turtleC_behind"
    </rosparam>
  </node>

  <!-- <node pkg="turtlebot_follow_turtlebot" type="turtlebot_follow_turtlebot" name="turtlebotD_follow_turtlebotC" output="screen">
    <rosparam>
        turtlebot   : "turtlebotE"
        target_turtlebot : "turtlebotD"
        target_turtlebot_behind : "turtleD_behind"
    </rosparam>
  </node> -->

</launch>
